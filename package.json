    "inquirer": "^5.1.0"
  },
  "devDependencies": {
    "bluebird": "^3.5.1",
    "request": "^2.83.0",
    "request-promise": "^4.2.2"
  },
  "pkg": {
    "targets": [
      "node8"
    ]
  }
}
